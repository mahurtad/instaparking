<%= simple_form_for @parking, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :id_parking %>
  <%= f.input :address %>
  <%= f.input :name %>
  <%= f.input :number %>
  <%= f.input :pricing_hour %>
  <%= f.input :image_one %>
  <%= f.input :image_two %>
  <%= f.input :latitude %>
  <%= f.input :longitude %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                parkings_path, :class => 'btn' %>
  </div>
<% end %>
